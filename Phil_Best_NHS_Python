   "source": [
    "# Reorder days of the week.\n",
    "day_order_wd = ['Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday']\n",
    "\n",
    "# Convert the column to a categorical type sorted in the above order\n",
