    "# Add a totals column\n",
    "apps_x_day_as_pivot['Total_apps'] = apps_x_day_as_pivot.sum(axis=1)\n",
    "\n",
    "# Add a totals row\n",
    "apps_x_day_as_pivot.loc[\"Total\"] = apps_x_day_as_pivot.sum()\n",
    "\n",
    "apps_x_day_as_pivot"
