   "source": [
    "# Aggregate on monthly level and determine the sum of records per month.\n",
    "nc_monthly_ss = nc.groupby(['appointment_month','service_setting'])[['count_of_appointments']]\\\n",
    ".sum().sort_values('appointment_month').reset_index()\n",
    "\n",
    "# View output.\n",
    "nc_monthly_ss"
   ]
  },
