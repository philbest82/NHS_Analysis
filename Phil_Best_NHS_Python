    "# Convert values to percentages\n",
    "# Create a new dataframe copying original\n",
    "apps_x_day_as_pct = apps_x_day_as_pivot.copy()\n",
    "\n",
    "# Convert values to percentages\n",
    "apps_x_day_as_pct[['Attended', 'DNA', 'Unknown']] = \\\n",
    "    apps_x_day_as_pct[['Attended', 'DNA', 'Unknown']]\\\n",
    "    .div(apps_x_day_as_pct['Total_apps'], axis=0) * 100\n",
    "\n",
    "apps_x_day_as_pct"
   ]
