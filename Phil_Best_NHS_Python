   "source": [
    "# Add a totals column\n",
    "nc_monthly_nc_pivot['Total_apps'] = nc_monthly_nc_pivot.sum(numeric_only=True, axis=1)\n",
    "\n",
    "# Add a totals row\n",
    "nc_monthly_nc_pivot.loc[\"Total\"] = nc_monthly_nc_pivot.sum(numeric_only=True)\n",
    "\n",
    "nc_monthly_nc_pivot"
