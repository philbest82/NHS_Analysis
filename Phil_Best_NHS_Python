   ],
   "source": [
    "# Aggregate on monthly level and determine the sum of records per month.\n",
    "nc_monthly_nc = nc.groupby(['appointment_month', 'national_category'])[['count_of_appointments']]\\\n",
    ".sum().sort_values('appointment_month').reset_index()\n",
    "\n",
    "# View output.\n",
    "nc_monthly_nc"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 191,
   "id": "75586b64-24a9-4c29-8220-0d57b4e39466",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Convert appointment_month to datetme64 so seaborn can read it\n",
    "nc_monthly_nc['appointment_month'] = nc_monthly_nc['appointment_month'].dt.to_timestamp()"
   ]
  },
