    "       'Unmapped', 'Unplanned Clinical Activity', 'Walk-in', 'Total_apps']]\\\n",
    "    .div(nc_monthly_nc_pct['Total_apps'], axis=0) * 100\n",
    "\n",
    "nc_monthly_nc_pct"
   ]
